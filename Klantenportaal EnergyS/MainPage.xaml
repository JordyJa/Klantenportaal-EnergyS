        <Image Name="Vragen" HorizontalAlignment="Left" Height="200" Width="200" VerticalAlignment="Top" Margin="223,428,0,0" Source="Assets/StoreLogo.png"/>
        <Image Name="Overzicht" HorizontalAlignment="Left" Height="200" Width="200" VerticalAlignment="Top" Margin="1011,428,0,0" Source="Assets/StoreLogo.png"/>
        <Image Name="Abonnement" HorizontalAlignment="Left" Height="200" Width="200" VerticalAlignment="Top" Margin="630,428,0,0" Source="Assets/StoreLogo.png"/>
        <Image Name="Profiel" HorizontalAlignment="Left" Height="50"  Width="50"  VerticalAlignment="Top" Margin="54,36,0,0" Source="Assets/StoreLogo.png"/>
