        <Button Name="Contactknop" Content="Contact opnemen" HorizontalAlignment="Left" Height="50" Width="150" VerticalAlignment="Bottom" Margin="251,0,0,294"/>
        <Button Name="Abonnementknop" Content="Naar abonnementen" HorizontalAlignment="Left" Height="50" Width="152" VerticalAlignment="Bottom" Margin="656,0,0,294"/>
        <Button Name="Overzichtknop" Content="Overzicht inzien" HorizontalAlignment="Left" Height="50" Width="150" VerticalAlignment="Bottom" Margin="1033,0,0,294"/>
